<mvc:View controllerName="rizobacter.despachos.controller.App"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <App>
        <pages>
            <Page title="{i18n>appTitle}">
            <headerContent>
                <Button id="helloDialogHeader" icon="sap-icon://action" press=".onOpenDialogHeader" class="sapUiSmallMarginEnd"/>
            </headerContent>
                <content>
                    <mvc:XMLView viewName="rizobacter.despachos.view.DespachosPanel"/>
                    <mvc:XMLView viewName="rizobacter.despachos.view.DespachosList" />
                </content>
            </Page>
        </pages>
    </App>
</mvc:View>